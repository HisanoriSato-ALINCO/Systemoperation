<p id="notice"><%= notice %></p>

<h1>Infosettings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @infosettings.each do |infosetting| %>
      <tr>
        <td><%= link_to 'Show', infosetting %></td>
        <td><%= link_to 'Edit', edit_infosetting_path(infosetting) %></td>
        <td><%= link_to 'Destroy', infosetting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Infosetting', new_infosetting_path %>
